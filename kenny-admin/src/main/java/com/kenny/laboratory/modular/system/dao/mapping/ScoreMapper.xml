<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kenny.laboratory.modular.system.dao.ScoreMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.kenny.laboratory.modular.system.model.Score">
        <id column="id" property="id" />
        <result column="student_id" property="studentId" />
        <result column="experiment_id" property="experimentId" />
        <result column="student_name" property="studentName" />
        <result column="experiment_name" property="experimentName" />
        <result column="attendance_num" property="attendanceNum" />
        <result column="score" property="score" />
        <result column="teacher_name" property="teacherName" />
        <result column="teacher_id" property="teacherId" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, student_id, experiment_id, student_name, experiment_name, attendance_num, score, teacher_name, teacher_id
    </sql>

</mapper>
